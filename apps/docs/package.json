{
  "name": "@evmts/docs",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" pnpm astro build",
    "test": "vitest",
    "preview": "astro preview",
    "check": "astro check",
    "tsc": "tsc",
    "format": "pnpm run format:code",
    "format:ci": "pnpm run format:imports && pnpm run format:code",
    "format:code": "prettier -w . --cache --plugin-search-dir=.",
    "format:imports": "organize-imports-cli ./tsconfig.json",
    "add-language": "node ./scripts/add-language.mjs",
    "docgen": "node ./scripts/docgen.mjs",
    "docgen:errors": "node ./scripts/error-docgen.mjs",
    "docgen:integrations": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/generate-integration-pages.ts",
    "lint:a11y": "start-test 'yarn build && yarn preview' 3000 'yarn lint:a11y:local'",
    "lint:a11y:local": "pa11y-ci --sitemap 'http://localhost:3000/sitemap.xml' --sitemap-find 'https://docs.astro.build' --sitemap-replace 'http://localhost:3000'",
    "lint:a11y:remote": "pa11y-ci --sitemap 'https://docs.astro.build/sitemap.xml'",
    "lint:linkcheck": "SKIP_OG=true pnpm build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "node ./scripts/lint-slugcheck.mjs",
    "lint:eslint": "eslint .",
    "github:translation-status": "node ./scripts/github-translation-status.mjs"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^3.0.0",
    "@actions/core": "^1.10.0",
    "@algolia/client-search": "^4.14.3",
    "@astrojs/mdx": "^0.17.0",
    "@astrojs/preact": "^2.0.3",
    "@astrojs/sitemap": "1.0.1",
    "@babel/core": "^7.20.12",
    "@docsearch/css": "^3.3.3",
    "@docsearch/react": "^3.3.3",
    "@types/chroma-js": "^2.4.0",
    "@types/hast": "^2.3.4",
    "@types/html-escaper": "^3.0.0",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "astro": "^2.0.14",
    "astro-auto-import": "^0.2.1",
    "astro-eslint-parser": "^0.11.0",
    "astro-og-canvas": "^0.1.6",
    "bcp-47-normalize": "^2.2.0",
    "canvaskit-wasm": "^0.38.0",
    "chroma-js": "^2.4.2",
    "dedent-js": "^1.0.1",
    "domhandler": "^5.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-react": "^7.32.2",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "hast-util-from-html": "^1.0.1",
    "hast-util-to-string": "^2.0.0",
    "hastscript": "^7.2.0",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^8.0.1",
    "kleur": "^4.1.5",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-to-hast": "^12.3.0",
    "mdast-util-to-string": "^3.1.1",
    "micromark-util-character": "^1.1.0",
    "micromark-util-symbol": "^1.0.1",
    "node-fetch": "^3.3.0",
    "organize-imports-cli": "^0.10.0",
    "p-retry": "^5.1.2",
    "parse-numeric-range": "^1.3.0",
    "preact": "^10.12.1",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
    "prompts": "^2.4.2",
    "rehype": "^12.0.1",
    "remark": "^14.0.2",
    "remark-directive": "^2.0.1",
    "simple-git": "^3.16.1",
    "tiny-glob": "^0.2.9",
    "tsm": "^2.3.0",
    "typescript": "^4.9.5",
    "unified": "^10.1.2",
    "unist-util-remove": "^3.1.1",
    "unist-util-visit": "^4.1.2",
    "vfile": "^5.3.7",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "^4.5.13",
    "@nanostores/preact": "^0.3.1",
    "@types/canvas-confetti": "^1.6.0",
    "canvas-confetti": "^1.6.0",
    "jsdoc-api": "^8.0.0",
    "nanostores": "^0.7.4",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-smartypants": "^2.0.0",
    "sass": "^1.58.3"
  },
  "engines": {
    "node": "^14.15.0 || >=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@7.14.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "vite"
      ]
    }
  }
}
