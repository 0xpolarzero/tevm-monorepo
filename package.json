{
  "name": "evmts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/evmts-monorepo.git"
  },
  "scripts": {
    "typecheck": "nx run-many --target=typecheck",
    "test": "nx run-many --target=test",
    "build": "nx run-many --target=build",
    "e2e": "nx e2e e2e",
    "serve": "nx run-many --target=serve",
    "serve:docker": "docker-compose up",
    "lint": "nx run-many --targets=lint,format",
    "lint:check": "nx run-many --target=lint:check,format:check",
    "release": "pnpm publish -r --access=public",
    "release:dry": "pnpm publish -r --access=public --dry-run",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm install --lockfile-only"
  },
  "license": "MIT",
  "packageManager": "pnpm@7.18.2",
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@nrwl/nx-cloud": "^16.0.5",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.27.5",
    "nx": "^16.0.1",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@nrwl/devkit": "^16.0.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/parser": "^5.59.1",
    "babel": "^6.23.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^43.1.1",
    "eslint-plugin-react-query": "^1.3.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "gitmoji-cli": "^8.1.1",
    "prettier": "^2.8.8",
    "rome": "^12.0.0",
    "ts-node": "^10.9.1",
    "wait-on": "^7.0.1"
  }
}
